{
  "extends": "tslint:recommended",
  "rules": {
    "no-shadowed-variable": false,
    "ban-types": [
      false
    ],
    "unified-signatures": [
      false
    ],
    "array-type": [
      false
    ],
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "only-arrow-functions": [
      false
    ],
    "prefer-for-of": false,
    "align": [
      false
    ],
    "no-console": [
      false
    ],
    "forin": false,
    "member-access": false,
    "member-ordering": [
      false
    ],
    "no-bitwise": false,
    "no-namespace": [
      false
    ],
    "no-string-literal": false,
    "object-literal-key-quotes": [
      false
    ],
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "switch-default": false,
    "variable-name": false,
    "space-before-function-paren": [
      true,
      {
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  }
}